{
  "name": "proxy-env-sync",
  "type": "module",
  "version": "1.0.2",
  "private": true,
  "description": "Synchronize system proxy settings with environment variables",
  "author": "ZeonXr",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build && node ./scripts/build.js",
    "build:skip-typecheck": "vite build && node ./scripts/build.js",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "dependencies": {
    "@qc2168/vite-plugin-utools": "^1.7.0",
    "@vueuse/core": "^13.5.0",
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.2",
    "@types/node": "^20.19.4",
    "@unocss/eslint-plugin": "66.1.0-beta.6",
    "@vitejs/plugin-vue": "^5.2.4",
    "eslint": "^9.30.1",
    "eslint-plugin-format": "^1.0.1",
    "typescript": "^5.8.3",
    "unocss": "^66.3.3",
    "utools-api-types": "^5.2.0",
    "vite": "^6.3.5",
    "vue-tsc": "^2.2.12"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 9000
  }
}
